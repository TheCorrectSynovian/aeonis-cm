{
	"schemaVersion": 1,
	"id": "aeonis-manager",
	"version": "${25.8b}",
	"name": "Aeonis Plus",
	"description": "A Companion and Expansion mod for AEONIS modpack!",
	"authors": [
		"Quantum_Creeper"
	],
	"contact": {
		"homepage": "https://www.youtube.com/@quantumcreeper",
		"sources": "https://github.com/TheCorrectSynovian/Aeonis-mod"
	},
	"license": "MIT",
	"icon": "assets/aeonis-manager/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.qc.aeonis.AeonisManager",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.qc.aeonis.AeonisManagerClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.qc.aeonis.AeonisManagerDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"aeonis-manager.mixins.json",
		{
			"config": "aeonis-manager.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}