{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.qc.aeonis",
		"id": "aeonis-command-master",
		"version": "${version}",
		"metadata": {
			"name": "Aeonis: Command Master",
			"description": "A powerful command utility mod! Take control of any mob, spawn custom entities, and unlock fun gameplay commands. Works standalone or as the perfect companion for modpacks. Features: Mob Possession, Custom Mobs (Copper Stalker & more), Admin Commands, and endless fun!",
			"contributors": {
				"Quantum_Creeper": "Author"
			},
			"contact": {
				"homepage": "https://www.youtube.com/@quantumcreeper",
				"sources": "https://github.com/TheCorrectSynovian/Aeonis-mod",
				"issues": "https://github.com/TheCorrectSynovian/Aeonis-mod/issues"
			},
			"license": "MIT",
			"icon": "assets/aeonis-manager/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": [
				{
					"value": "com.qc.aeonis.AeonisManager",
					"adapter": "kotlin"
				}
			],
			"client_init": [
				{
					"value": "com.qc.aeonis.AeonisManagerClient",
					"adapter": "kotlin"
				}
			]
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0"
			},
			{
				"id": "quilted_fabric_api",
				"versions": "*"
			},
			{
				"id": "minecraft",
				"versions": ">=1.21.11"
			},
			{
				"id": "java",
				"versions": ">=21"
			}
		],
		"provides": [
			{
				"id": "aeonis-command-master",
				"version": "${version}"
			}
		]
	},
	"mixin": [
		"aeonis-manager.mixins.json",
		"aeonis-manager.client.mixins.json"
	]
}
